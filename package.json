{
  "name": "xpose",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "start": "npm run start:dev",
    "start:dev": "vue-cli-service serve --mode development",
    "start:test": "vue-cli-service serve --mode test",
    "start:online": "vue-cli-service serve --mode production",
    "lint": "vue-cli-service lint",
    "serve:dev": "npx vue-cli-service inspect --mode development >> webpack.config.development.js",
    "serve:pro": "npx vue-cli-service inspect --mode production >> webpack.config.production.js"
  },
  "dependencies": {
    "awe-dnd": "^0.3.4",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "element-ui": "^2.14.1",
    "jquery": "^3.5.1",
    "jquery-ui-dist": "^1.12.1",
    "qrcodejs2": "0.0.2",
    "vue": "^2.6.11",
    "vue-draggable-resizable-gorkys": "^2.4.4",
    "vue-router": "^3.2.0",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
